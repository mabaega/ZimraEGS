    {
      "Key": "75190339-992a-4fb0-bf79-f5f8226edb4b",
      "Name": "Receipt Reference",
      "Description": "<script>\ndocument.addEventListener('DOMContentLoaded', function () {\n    const MakeHidden = labels => updateFields(labels, 'hide');\n\tconst MakeReadonly = labels => updateFields(labels, 'readonly');\n\tconst MakeReadonlyAndHide = labels => updateFields(labels, 'readonlyandhide');\n\tfunction updateFields(labels, action) {\n\t\tconst customFieldset = [...document.querySelectorAll('fieldset')].find(fieldset => fieldset.querySelector('legend')?.textContent.trim() === 'Custom Fields');\n\t\tif (!customFieldset) return console.error('Custom Fields fieldset not found');\n\t\tcustomFieldset.querySelectorAll('.form-group').forEach(group => {\n\t\t\tconst label = group.querySelector('label'), input = group.querySelector('input, textarea'), dropdown = group.querySelector('.vs__dropdown-toggle');\n\t\t\tif (label && labels.includes(label.textContent.trim())) {\n\t\t\t\tif (action === 'hide') group.style.display = 'none';\n\t\t\t\telse if (action === 'readonly' && input) input.setAttribute('readonly', 'true');\n\t\t\t\telse if (action === 'readonlyandhide') { if (input) input.setAttribute('readonly', 'true'); group.style.display = 'none'; }\n\t\t\t\tif (dropdown) {\n\t\t\t\t\tconst searchInput = dropdown.querySelector('input.vs__search'), clearButton = dropdown.querySelector('.vs__clear');\n\t\t\t\t\tif (action === 'readonly' || action === 'readonlyandhide') {\n\t\t\t\t\t\tdropdown.setAttribute('disabled', 'true');\n\t\t\t\t\t\tif (searchInput) searchInput.setAttribute('readonly', 'true');\n\t\t\t\t\t\tif (clearButton) clearButton.style.display = 'none';\n\t\t\t\t\t\tdropdown.style.pointerEvents = 'none';\n\t\t\t\t\t}\n\t\t\t\t\tif (action === 'hide' || action === 'readonlyandhide') dropdown.style.pointerEvents = 'none';\n\t\t\t\t}\n\t\t\t}\n\t\t});\n\t}\n    MakeReadonly(['Receipt QRCode', 'Verification Code', 'Approval Status', 'Receipt Reference']);\n    MakeReadonlyAndHide(['Device ID', 'Device SN','ReceiptCounter','ReceiptGlobalNo','FiscalDayNo']);\n});\n</script>",
      "Placement": [
